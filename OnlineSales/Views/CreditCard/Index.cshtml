@using OnlineSales.DL

@model List<CreditCard>

@{
    ViewBag.Title = "CreditCardList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>CreditCardList</h2>

<p>
    <a href="/CreditCard/RegisterCreditCard"> <img src="data:image/png;base64," width="20" height="20"></a>
</p>
<table class="table">
    <tr>
        <th>
            <p>Number</p>
        </th>
        <th>
            <p>Valid tru</p>
        </th>
        <th>
            <p>CVV</p>
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Number)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ValidTru)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CVV)
        </td>
        <td>
            @Html.ActionLink("Delete", "DeleteCreditCard", new { id = item.ID })
        </td>
    </tr>
}

</table>
